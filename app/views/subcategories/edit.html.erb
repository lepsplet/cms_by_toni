<h2>Spremeni naslov podkategorije</h2>
<%= bootstrap_form_for @subcategory do |f| %>

	<%= render 'error_messages' %>

	<div><%= f.text_field :name, label: 'Ime:',  autofocus: true %></div>
	<div><%= f.submit 'shrani', class: 'btn btn-primary btn-lg' %></div>
<% end %>

<%= link_to 'nazaj', { action: :index, id: @subcategory.category_id }, class: 'btn btn-default' %>
